    "lines": "./scripts/lines.sh",
    "lint": "./scripts/lint.sh",
    "compile": "./scripts/compile.sh",
    "build": "./scripts/build.sh",
    "prepush": "./scripts/lint.sh && ./scripts/test.sh",
    "check": "./scripts/check.sh",
    "dbuild": "npm run bundle && docker build -t ops .",
    "dup": "docker run --name ops -d -p 1337:1337 ops && docker start ops",
    "ddown": "docker rm -f ops",
    "dexec": "docker exec -ti ops /bin/sh",
    "smoke-setup": "./scripts/smoke-setup.sh",
    "presmoke": "docker start selenium-firefox &>/dev/null",
    "smoke": "nightwatch -c ./smoke/nightwatch.json",
    "postsmoke": "docker stop selenium-firefox &>/dev/null",
    "setup": "./scripts/setup.sh"
  },
